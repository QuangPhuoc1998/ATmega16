
#include "main.h"

#define RS PORTD.0
#define EN PORTD.1
#define D4 PORTD.2
#define D5 PORTD.3
#define D6 PORTD.4
#define D7 PORTD.5

void Lcd_Port(char a);
void Lcd_Cmd(char a);
void Lcd_Clear();
void Lcd_Set_Cursor(char a, char b);
void Lcd_Init();
void Lcd_Write_Char(char a);
void Lcd_Write_String(char *a);
void Lcd_Shift_Right();
void Lcd_Shift_Left();